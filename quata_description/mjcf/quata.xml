    <mesh name="lowerLegA1" file="lowerLegA1.STL"/>
    <mesh name="lowerLegA2" file="lowerLegA2.STL"/>
    <mesh name="lowerHukeA" file="lowerHukeA.STL"/>

    <mesh name="upperLegB" file="upperLegB.STL"/>
